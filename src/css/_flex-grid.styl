.f
  $this = selector()

  &
  &-box
  &-grid
  &-fill
    display flex

  &-fill
    > *
      flex 1 0 auto

  &-align
    $base = selector()
    /
      flex-alignment($base)

  &-wrap
    flex-wrap wrap

  &-gap
    for $i in 1..10
      &-{$i}
        flex-gap($i)

  &-v-gap
    for $i in 1..10
      &-{$i}
        $gap = $base-gap * $i
        margin-bottom - $gap
        > *
          margin-bottom $gap
